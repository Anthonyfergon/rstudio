<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
   xmlns:g="urn:import:com.google.gwt.user.client.ui"
   xmlns:rw="urn:import:org.rstudio.core.client.widget">
   <ui:with field="res" type="org.rstudio.studio.client.panmirror.dialogs.PanmirrorDialogsResources" />
   <ui:with field="ElementIds" type="org.rstudio.core.client.ElementIds"/>
   <ui:style>
   
      
   </ui:style>
   
   <g:VerticalPanel styleName="{res.styles.dialog}">
   
      <rw:FormLabel for="{ElementIds.getVisualMdCitationId}" text="Citation ID (e.g. @Knuth):"/>
      <rw:FormTextBox elementId="{ElementIds.getVisualMdCitationId}" styleName="{res.styles.fullWidth} {res.styles.spaced}" ui:field="citationId_"/>
    
      <rw:FormLabel for="{ElementIds.getVisualMdCitationLocator}" text="Locator (e.g. pp. 33-35):"/>
      <rw:TextBoxWithCue elementId="{ElementIds.getVisualMdCitationLocator}" cueText="(Optional)" styleName="{res.styles.fullWidth} {res.styles.spaced}" ui:field="locator_"/>
      	
   </g:VerticalPanel>

</ui:UiBinder> 
